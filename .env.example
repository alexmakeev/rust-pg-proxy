# Upstream PostgreSQL (production)
UPSTREAM_HOST=your-postgres-host
UPSTREAM_PORT=5432
UPSTREAM_USER=your-user
UPSTREAM_PASSWORD=your-password
UPSTREAM_DATABASE=your-database

# Proxy settings
LISTEN_HOST=0.0.0.0
LISTEN_PORT=5433

# Cache settings
CACHE_TTL_SECONDS=900
MAX_CACHE_ENTRIES=10000

# Connection pool
POOL_SIZE=5

# Logging
RUST_LOG=info
